local function UpdateStatus(aa,ab)
C.Text=aa
C.TextColor3=ab
end

local function ButtonHover(aa,ab,W)
local X=aa:FindFirstChild"DiscordBtnText"
if X then
aa.MouseEnter:Connect(function()
Tween(X,TweenInfo.new(0.2,Enum.EasingStyle.Sine),{TextTransparency=W}):Play()
end)
aa.MouseLeave:Connect(function()
Tween(X,TweenInfo.new(0.2,Enum.EasingStyle.Sine),{TextTransparency=ab}):Play()
end)
end
end

local aa=false

local function VerifyKey()

if aa then
return
end

local ab=D.Text:gsub("%s+","")
if ab==""then
UpdateStatus("Please enter a key!",Color3.fromRGB(255,100,100))
task.wait(2)
UpdateStatus(b.Description,Color3.fromRGB(255,255,255))
return
end

aa=true
UpdateStatus("Checking key...",Color3.fromRGB(255,200,100))
K.Text="Checking..."

task.spawn(function()
local W,X=checkKey(ab)

if W then
local Y=detectVersion()
UpdateStatus("Key valid! Loading...",Color3.fromRGB(100,255,100))

getgenv().script_key=ab
getgenv().HoN_Version=Y


if writefile then
pcall(function()
writefile(g,ab)
end)
end

local Z=Y=="normal"and"PREMIUM"or("PREMIUM | "..Y:upper())

game:GetService"StarterGui":SetCore("SendNotification",{
Title=b.Title;
Text=d.name.." | "..Z.." loaded!";
Icon="rbxassetid://123156553209294";
Duration=5;
})

m:Destroy()
loadScript()
else
local Y={
KEY_HWID_LOCKED="Key locked to different device",
KEY_INCORRECT="Invalid key",
KEY_EXPIRED="Key expired",
KEY_BANNED="Key banned",
KEY_INVALID="Invalid key format",
SCRIPT_ID_INCORRECT="Script ID error",
SCRIPT_ID_INVALID="Invalid script ID",
INVALID_EXECUTOR="Unsupported executor",
INVALID_VERSION="Version not supported"
}

local Z=Y[X.code]or("Error: "..tostring(X.message))
UpdateStatus(Z,Color3.fromRGB(255,100,100))
K.Text="Verify Key"
aa=false
task.wait(2)
UpdateStatus(b.Description,Color3.fromRGB(255,255,255))
end
end)
end

ButtonHover(N,0.5,0.3)




x.MouseButton1Click:Connect(function()
m:Destroy()
end)

N.MouseButton1Click:Connect(function()
setclipboard(b.DiscordLink)
UpdateStatus("Discord link copied!",Color3.fromRGB(100,255,100))
task.wait(2)
UpdateStatus(b.Description,Color3.fromRGB(255,255,255))
end)

H.MouseButton1Click:Connect(function()
VerifyKey()
end)

D.FocusLost:Connect(function(ab)
if ab then
VerifyKey()
end
end)




local ab,W,X,Y

q.InputBegan:Connect(function(Z)
if Z.UserInputType==Enum.UserInputType.MouseButton1 then
ab=true
X=Z.Position
Y=n.Position

Z.Changed:Connect(function()
if Z.UserInputState==Enum.UserInputState.End then
ab=false
end
end)
end
end)

q.InputChanged:Connect(function(Z)
if Z.UserInputType==Enum.UserInputType.MouseMovement then
W=Z
end
end)

j.InputChanged:Connect(function(Z)
if Z==W and ab then
local _=Z.Position-X
n.Position=UDim2.new(Y.X.Scale,Y.X.Offset+_.X,Y.Y.Scale,Y.Y.Offset+_.Y)
end
end)




