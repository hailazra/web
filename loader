local function loadScriptByTier(g)local h=g=='premium'and d.premium_script_id or
d.freemium_script_id f.script_id=h local i=getgenv().script_key or _G.script_key
if i then pcall(function()script_key=i end)pcall(function()_G.script_key=i end)
end local j,k=pcall(function()f.load_script()end)if j then return true else
warn('Failed to load script:',k)return false,k end end local function
determineKeyTier(g)f.script_id=d.premium_script_id local h,i=pcall(function()
return f.check_key(g)end)if h and i.code=='KEY_VALID'then return'premium',i end
if h and(i.code=='KEY_INCORRECT'or i.code=='KEY_INVALID')then f.script_id=d.
freemium_script_id local j,k=pcall(function()return f.check_key(g)end)if j and k
.code=='KEY_VALID'then return'freemium',k end return nil,k end return nil,i end
