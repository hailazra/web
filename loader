local e,f=pcall(function()
return loadstring(game:HttpGet"https://sdkapi-public.luarmor.net/library.lua")()
end)

if not e then
warn("Failed to load Luarmor library:",f)
return
end




local function detectVersion()


local g=getgenv().HoN_Version or rawget(_G,"Version")or _G.Version or"normal"


if type(g)~="string"then
g=rawget(_G,"Version")or _G.Version or"normal"
end

g=tostring(g):lower()


if g~="kaitun"and g~="lite"then
g="normal"
end


warn("DETECTED VERSION:",g:upper())

return g
end




local function loadScript()
local g=getgenv().HoN_Version or detectVersion()
local h=d.script_ids[g]

if not h then
warn("No script_id found for version:",g)
return false,"Invalid version"
end


warn("LOADING VERSION:",g:upper())
warn("SCRIPT_ID:",h)

f.script_id=h


local i=getgenv().script_key or _G.script_key
if i then
pcall(function()script_key=i end)
pcall(function()_G.script_key=i end)
end

local j,k=pcall(function()
f.load_script()
end)

if j then
warn"âœ… Script loaded successfully!"
return true
else
warn("Failed to load script:",k)
return false,k
end
end

local function checkKey(g)
local h=detectVersion()
local i=d.script_ids[h]

if not i then
warn("No script_id found for version:",h)
return false,{code="INVALID_VERSION",message="Version not supported"}
end

f.script_id=i
local j,k=pcall(function()
return f.check_key(g)
end)

if j and k.code=="KEY_VALID"then
return true,k
end

return false,k
end




local g="NoctisPremium.txt"
local h="NoctisKey"..c..".txt"

local function checkSavedKey()
local i


pcall(function()
if script_key and script_key~=""then
i=script_key
end
end)


if not i then
i=rawget(_G,"script_key")or _G.script_key
end


if not i then
i=getgenv().script_key
end


if not i and isfile and readfile then
local j,k=pcall(function()
return readfile(g)
end)
if j and k and k~=""then
i=k:gsub("%s+","")
end
end


if not i and isfile and readfile then
local j,k=pcall(function()
return readfile(h)
end)
if j and k and k~=""then
i=k:gsub("%s+","")


if writefile then
pcall(function()
writefile(g,i)
end)
end


if delfile then
pcall(function()
delfile(h)
end)
end
end
end


if i then
i=i:gsub("%s+","")
end

if i and i~=""then local
j=checkKey(i)

if j then
local k=detectVersion()


pcall(function()script_key=i end)
pcall(function()_G.script_key=i end)
pcall(function()getgenv().script_key=i end)
getgenv().HoN_Version=k


if isfile and writefile then
pcall(function()
writefile(g,i)
end)
end

local l=k=="normal"and"PREMIUM"or("PREMIUM | "..k:upper())

game:GetService"StarterGui":SetCore("SendNotification",{
Title="Key Valid!";
Text=d.name.." | "..l;
Icon="rbxassetid://123156553209294";
Duration=3;
})

loadScript()
return true
else

if isfile and delfile then
pcall(function()
if isfile(g)then
delfile(g)
end
if isfile(h)then
delfile(h)
end
end)
end
end
end

return false
end





if rawget(_G,"script_key")and not getgenv().script_key then
getgenv().script_key=rawget(_G,"script_key")
warn"Normalized script_key from _G"
elseif _G.script_key and not getgenv().script_key then
getgenv().script_key=_G.script_key
warn"Normalized script_key from _G"
end


if rawget(_G,"Version")and type(rawget(_G,"Version"))=="string"and not getgenv().HoN_Version then
getgenv().HoN_Version=rawget(_G,"Version")
warn("Normalized Version from _G:",getgenv().HoN_Version)
elseif _G.Version and type(_G.Version)=="string"and not getgenv().HoN_Version then
getgenv().HoN_Version=_G.Version
warn("Normalized Version from _G:",getgenv().HoN_Version)
end


warn("Final _G.Version:",_G.Version or"nil")
warn("Final getgenv().HoN_Version:",getgenv().HoN_Version or"nil")


if checkSavedKey()then
return
end




